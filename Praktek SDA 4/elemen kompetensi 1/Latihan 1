from numpy import random
print("")

print("Elemen Kompetensi 1 Latihan 1")
print("===Rafli Riansyah===")
one=[]
ask=int(input("Masukan angka: "))
for i in range(ask):
    one.append(random.randint(1,100))
    one.sort()
print("Ini listnya =",one)
